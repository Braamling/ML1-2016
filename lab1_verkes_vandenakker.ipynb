    "\n",
    "def gen_sinusoidal(N):\n",
    "    sigma = 0.2\n",
    "    x = np.random.uniform(0,2*np.pi, N)\n",
    "    mu = np.sin(x)\n",
    "    t = np.random.normal(mu, sigma, N)\n",
    "    \n",
    "    return x, t\n",
    "\n",
    "N=100\n",
    "\n",
    "x, t = gen_sinusoidal(N)\n",
    "\n",
    "plt.plot(x, t, 'b.')\n",
    "y = np.linspace(0, 2 * np.pi, N)\n",
    "plt.plot(y, np.sin(y), 'r-')\n",
    "plt.show()"
   ]
